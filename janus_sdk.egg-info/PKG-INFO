    if name:
        await ctx.send(f"ã“ã‚“ã«ã¡ã¯ã€{name}ã•ã‚“ï¼")
    else:
        await ctx.send("ã“ã‚“ã«ã¡ã¯ï¼")

@bot.command(name="weather")
